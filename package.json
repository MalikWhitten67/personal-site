{
  "name": "personal-website",
  "version": "1.2.0",
  "description": "Personal website",
  "scripts": {
    "compile": "npx vaderjs@latest --build",
    "build:css": "postcss src/css/dev.css -o dist/public/css/styles.css",
    "build": "npx vaderjs@latest --build &&  tailwindcss -i src/css/dev.css -o ./dist/public/css/styles.css --watch"
  },
  "dependencies": {
    "daisyui": "^4.6.0",
    "postcss-cli": "^11.0.0",
    "vaderjs": "latest"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  }
}
